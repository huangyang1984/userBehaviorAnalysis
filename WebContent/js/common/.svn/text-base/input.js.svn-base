function replaceNotNumber(obj)
{
	var pattern = /[^0-9]/g;
	if(pattern.test(obj.value))
	{
		obj.value = obj.value.replace(pattern,"");
	}
}

function replaceChinese(obj)
{
	var pattern = /[^0-9a-zA-Z]/g;
	if(pattern.test(obj.value))
	{
		obj.value = obj.value.replace(pattern,"");
	}
}

