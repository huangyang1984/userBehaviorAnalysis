<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class dynamic-update="true" table="x27_accessory_content" name="net.gbicc.x27.core.model.AccessoryContent" schema="tcrcb">
    <id type="string" column="accessory_content_id" length="32" name="idStr">
      <generator class="uuid"/>
    </id>
    <property name="content" not-null="true" type="binary" column="content"/>
  </class>
</hibernate-mapping>
